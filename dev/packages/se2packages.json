  "macros": {
    "makebibmarginnote{}": {
      "snippet": "makebibmarginnote{${1:arg}}",
      "option": "bibdev=numeric"
    },
    "maycite[][]{}": {
      "snippet": "maycite[${2:prenote}][${3:postnote}]{${1:bibid}}",
      "option": "bibdev=numeric"
    },
    "maycite[]{}": {
      "snippet": "maycite[${2:postnote}]{${1:bibid}}",
      "option": "bibdev=numeric"
    },
    "maycite{}": {
      "snippet": "maycite{${1:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincite[][]{}": {
      "snippet": "margincite[${2:prenote}][${3:postnote}]{${1:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincite[]{}": {
      "snippet": "margincite[${2:postnote}]{${1:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincite{}": {
      "snippet": "margincite{${1:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites{}{}": {
      "snippet": "margincites{${1:bibid}}{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites(){}{}": {
      "snippet": "margincites(${3:post}){${1:bibid}}{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites()(){}{}": {
      "snippet": "margincites(${3:pre})(${4:post}){${1:bibid}}{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites()()[]{}[]{}": {
      "snippet": "margincites(${5:pre})(${6:post})[${3:post}]{${1:bibid}}[${4:post}]{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites[]{}[]{}": {
      "snippet": "margincites[${3:post}]{${1:bibid}}[${4:post}]{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites[][]{}[][]{}": {
      "snippet": "margincites[${3:pre}][${4:post}]{${1:bibid}}[${5:pre}][${6:post}]{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "margincites()()[][]{}[][]{}": {
      "snippet": "margincites(${7:pre})(${8:post})[${3:pre}][${4:post}]{${1:bibid}}[${5:pre}][${6:post}]{${2:bibid}}",
      "option": "bibdev=numeric"
    },
    "bibliofont": {
      "option": "bibdev=numeric"
    }
  },
