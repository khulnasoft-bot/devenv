{
    "name": "pweave",
    "patterns": [
        {
            "include": "text.tex.dev"
        },
        {
            "begin": "^(\\s*?)(<<)(.*?)(>>=)\\s*(.+)?\\s*\\n",
            "beginCaptures": {
                "2": {
                    "name": "meta.tag.pweave"
                },
                "3": {
                    "patterns": [
                        {
                            "include": "source.python"
                        }
                    ]
                },
                "4": {
                    "name": "meta.tag.pweave"
                },
                "5": {
                    "name": "invalid.illegal.pweave"
                }
            },
            "contentName": "source.python",
            "end": "^\\1(@)\\s*(.+)?\\s*\\n",
            "endCaptures": {
                "1": {
                    "name": "meta.tag.pweave"
                },
                "2": {
                    "name": "invalid.illegal.pweave"
                }
            },
            "name": "text.tex.dev.pweave.codeblock",
            "patterns": [
                {
                    "include": "source.python"
                }
            ]
        },
        {
            "captures": {
                "1": {
                    "name": "meta.tag.pweave"
                },
                "2": {
                    "patterns": [
                        {
                            "include": "source.python"
                        }
                    ]
                },
                "3": {
                    "name": "meta.tag.pweave"
                }
            },
            "match": "(<%=?)\\s*(.*?)\\s*(%>)",
            "name": "text.tex.dev.pweave.inlinecode"
        }
    ],
    "scopeName": "text.tex.dev.pweave"
}